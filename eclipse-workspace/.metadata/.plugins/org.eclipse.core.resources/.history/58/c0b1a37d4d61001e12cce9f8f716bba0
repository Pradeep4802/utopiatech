package practice.multithreaded;

public class RunnableDemo_02_2 implements Runnable{
	
	public void run() {
		for(int i = 0;i < 5;i++) {
			System.out.println("Executed : "+i);
			try {
				Thread.sleep(300);
			} catch(Exception e) {
				e.printStackTrace();
			}
		}
	}
	
	public static void main(String[] args) {
		
	}
}
