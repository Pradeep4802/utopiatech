package practice.basic;

public class Static_01_10_1 {
	public static void show() {
		System.out.println("System Show");
	}
	public void display() {
		System.out.println("System Display");
	}
	
	public static void main(String[] args) {
		Static_01_10_1 st1 = new Static_01_10_1();
		st1.show();
		st1.display();
	}
}
