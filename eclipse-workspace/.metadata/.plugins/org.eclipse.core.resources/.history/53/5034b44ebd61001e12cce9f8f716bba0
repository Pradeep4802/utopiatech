package com.Pattern;

public class NumberPattern_1 {
	public static void FirstPattern() {
		int i = 1;
		while(i<=5) {
			int j = 5;
			while(j>i) {
				System.out.println(" ");
				j--;
			}
			int k = i;
			while(k >= i) {
				System.out.println(k);
				k--;
			}
			i++;
		}
	}
	public static void main(String[] args) {
		FirstPattern();
	}
}
