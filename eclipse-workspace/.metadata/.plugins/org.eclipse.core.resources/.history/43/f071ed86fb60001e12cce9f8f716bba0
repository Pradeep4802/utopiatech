package com.lambda;

public class ImplementLambda {
	
	public static void main(String[] args) {
		LambdaDemo lm1 = new LambdaDemo() {
			
			public void demo() {
				System.out.println("Demo Executed");
			}
		};
		lm1.demo();
		Demon_1 d1 = (a,b)->{
			return a+b;
		};
		System.out.println(d1.add(23, 59));
	}
}
